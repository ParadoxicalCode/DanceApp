                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                        </Grid>
                    </Border>

                    <Border Style="{StaticResource Border}" Grid.Column="1">
                        <TextBlock Text="Пары" Style="{StaticResource Text}" FontSize="14"/>
                    </Border>

                    <Border Style="{StaticResource Border}" Grid.Row="1" Grid.Column="1">
