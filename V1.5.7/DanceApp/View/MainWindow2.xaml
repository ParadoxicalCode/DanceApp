<Window x:Class="DanceApp.View.MainWindow2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DanceApp.View" mc:Ignorable="d"
        MinHeight="720" Height="720" MinWidth="1150" Width="1150" WindowStartupLocation="CenterScreen">

    <Window.CommandBindings>
        <CommandBinding Command="{StaticResource Navigate}" Executed="OnNavigate"/>
    </Window.CommandBindings>

    <Frame Name="MainFrame" Source="/View/MainPageView.xaml" Background="White" 
           NavigationUIVisibility="Hidden" Margin="5,0,5,5"/>
</Window>