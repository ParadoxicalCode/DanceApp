<Window x:Class="DanceApp.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:DanceApp"
    mc:Ignorable="d"
    Height="600" MinHeight="500" Width="960" MinWidth="800">
    
    <Window.CommandBindings>
        <CommandBinding Command="{StaticResource Navigate}" Executed="OnNavigate"/>
    </Window.CommandBindings>

    <Frame Name="MainFrame" Source="/View/Page1/Page1View.xaml" Background="White" 
           NavigationUIVisibility="Hidden" Margin="10"/>
</Window>
